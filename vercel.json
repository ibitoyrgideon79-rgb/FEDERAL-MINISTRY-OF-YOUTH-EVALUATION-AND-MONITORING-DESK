{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "ADMIN_EMAIL": "@admin_email",
    "EMAIL_BACKEND": "@email_backend",
    "SMTP_HOST": "@smtp_host",
    "SMTP_PORT": "@smtp_port",
    "SMTP_USERNAME": "@smtp_username",
    "SMTP_PASSWORD": "@smtp_password",
    "FROM_EMAIL": "@from_email"
  }
}
